{
    "name": "payavel/payavel",
    "description": "A robust gateway that integrates with any payment processor and Laravel.",
    "license": "MIT",
    "authors": [
        {
            "name": "Robert Kujawa",
            "email": "robert.kujawa@proton.me"
        }
    ],
    "require-dev": {
        "orchestra/testbench": "^6.21"
    },
    "autoload": {
        "psr-4": {
            "Payavel\\": "src/",
            "Payavel\\Database\\Factories\\": "src/database/factories/",
            "Payavel\\Database\\Seeders\\": "src/database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Payavel\\Tests\\": "tests/",
            "Payavel\\Tests\\App\\": "tests/app/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Payavel\\PaymentServiceProvider"
            ]
        }
    }
}
